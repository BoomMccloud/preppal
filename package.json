{
  "name": "preppal",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "dev:worker": "wrangler dev --config worker/wrangler.toml",
    "build": "next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "check": "next lint && tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "test": "vitest",
    "test:ci": "vitest run",
    "proto:generate": "rm -rf src/lib/proto worker/src/lib/proto && buf generate",
    "podman:build": "podman build --no-cache --platform linux/amd64 -f docker/Dockerfile --build-arg NEXT_PUBLIC_WORKER_URL=\"$(grep NEXT_PUBLIC_WORKER_URL .env | cut -d '=' -f2)\" -t docker.io/boommccloud/preppal:latest .",
    "podman:push": "podman push docker.io/boommccloud/preppal:latest"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@bufbuild/protobuf": "^2.10.2",
    "@google/genai": "^1.28.0",
    "@prisma/client": "^6.5.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "jose": "^6.1.0",
    "next": "15.5.9",
    "next-auth": "5.0.0-beta.25",
    "next-intl": "^4.6.1",
    "protobufjs": "^7.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "ws": "^8.18.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.61.0",
    "@bufbuild/protoc-gen-es": "^2.10.2",
    "@cloudflare/workers-types": "^4.20251014.0",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^17.2.3",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "jsdom": "^27.0.0",
    "miniflare": "^4.20251011.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.5.0",
    "protobufjs-cli": "^1.1.3",
    "tailwindcss": "^4.0.15",
    "tsx": "^4.20.6",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.45.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "pnpm@10.15.0"
}
