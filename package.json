{
  "name": "preppal",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo",
    "dev:ws": "tsx src/server/ws/server.ts",
    "dev:worker": "wrangler dev --config worker/wrangler.toml",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "proto:generate": "mkdir -p src/lib && pbjs -t static-module -w es6 -o src/lib/interview_pb.js proto/interview.proto && pbts -o src/lib/interview_pb.d.ts src/lib/interview_pb.js && cp src/lib/interview_pb.js worker/src/lib/ && cp src/lib/interview_pb.d.ts worker/src/lib/",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run",
    "db:seed": "tsx prisma/seed.ts",
    "debug:list-interviews": "tsx scripts/list-interviews.ts",
    "debug:check-status": "tsx scripts/check-interview-status.ts",
    "debug:update-status": "tsx scripts/update-interview-status.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@google/genai": "^1.28.0",
    "@prisma/client": "^6.5.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "jose": "^6.1.0",
    "next": "15.5.9",
    "next-auth": "5.0.0-beta.25",
    "next-intl": "^4.6.1",
    "protobufjs": "^7.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "ws": "^8.18.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251014.0",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^17.2.3",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "jsdom": "^27.0.0",
    "miniflare": "^4.20251011.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.5.0",
    "protobufjs-cli": "^1.1.3",
    "tailwindcss": "^4.0.15",
    "tsx": "^4.20.6",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.45.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "pnpm@10.15.0"
}
